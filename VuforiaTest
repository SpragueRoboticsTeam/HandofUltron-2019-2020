    /****************************** shutdown ***************************/
    /****************************** shutdown ***************************/
    // Turn the motor power off and shutdown the TensorFlow Object Detection Engine
    public void shutdown()
    {
        telemetry.addData("State: ", "Done");

        left.setPower(0.0);
        right.setPower(0.0);
        front.setPower(0.0);
        back.setPower(0.0);

        if (myTfod != null)
        {
            myTfod.shutdown();
        }

        telemetry.update();
    }
}

